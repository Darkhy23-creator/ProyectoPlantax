@page "/"
@inject IStringLocalizer<Resource> localizer

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta content="text/html; charset=iso-8859-2" http-equiv="Content-Type">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

    <style>
        .mySlides {
            display: none
        }

        .w3-left, .w3-right, .w3-badge {
            cursor: pointer
        }

        .w3-badge {
            height: 13px;
            width: 13px;
            padding: 0
        }
    </style>
</head>



<body>


    <!-- Page content -->
    <div class="w3-content" style="max-width:2000px;margin-top:46px">

        <div class="w3-display-container w3-center">
            <img src="https://i.pinimg.com/originals/41/53/90/41539031115b7ec46e55f994fb1277a1.jpg" class="w3-opacity-min w3-round " style="width:100%">
            <div class="w3-display-middle w3-container w3-whithe w3-text-black w3-padding-32 w3-hide-small">
                <h2 class="w3-wide">PLANTAX</h2>
                <p><i>@localizer["Greting2"]</i></p>
                <p class="w3-justify">@localizer["message1"]</p>
            </div>
        </div>

        <section class="main">
            <div class="w3-panel w3-amber w3-card-4">
                <h2 class="w3-wide w3-center">@localizer["Video"]</h2>
            </div>
            <div class=" w3-display-middl w3-container w3-center "
                <iframe width="560" height="315" src="https://www.youtube.com/embed/g-ELzZfo0RA" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write;
                        encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>

            </div>

        </section>




        <div class="w3-container w3-content w3-center w3-padding-64" style="max-width:800px" id="band">

            <div class="w3-panel w3-deep-orange w3-card-4">
                <h2 class="w3-wide w3-center">@localizer["EquipoTrabajo"] </h2>
            </div>

            <div class="w3-row w3-padding-32">
                <div class=" w3-card-4 w3-deep-purple w3-third">
                    <div class="w3-container w3-center">
                        <img src="https://i.pinimg.com/originals/b0/14/0e/b0140eeeaacd5c810e20bc67c799c823.png" class="w3-round w3-center" alt="Random Name" style="width:60%">
                        <p>Sebastian Caviedes</p>
                        <p>@localizer["Estudiante"]</p>
                    </div>
                </div>
                <div class=" w3-card-4 w3-amber w3-third">
                    <div class="w3-container w3-center">
                        <img src="https://i.pinimg.com/originals/dc/a6/b0/dca6b0e9b63e09fd3fd2c44029df0b99.png" class="w3-round w3-center" alt="Random Name" style="width:60%">
                        <p>Kevin Mahecha</p>
                        <p>@localizer["Estudiante"]</p>
                    </div>
                </div>
                <div class=" w3-card-4 w3-lime w3-third">
                    <div class="w3-container w3-center">
                        <img src="https://www.cheats.co/blog/wp-content/uploads/2013/11/05-H3u6wtD.jpg" class="w3-round w3-center" alt="Random Name" style="width:60%">
                        <p>Miguel Laguna</p>
                        <p>@localizer["Estudiante"]</p>
                    </div>
                </div>
                <div class=" w3-card-4 w3-indigo w3-third">
                    <div class="w3-container w3-center">
                        <img src="https://payload.cargocollective.com/1/0/399/6826828/XBOX_gp_hlantern.png" class="w3-round w3-center" alt="Random Name" style="width:60%">
                        <p>Stiven Acero</p>
                        <p>@localizer["Estudiante"]</p>
                    </div>
                </div>
                <div class=" w3-card-4 w3-teal w3-third">
                    <div class="w3-container w3-center">
                        <img src="https://live.staticflickr.com/5550/12110536015_c4bb1f3e95_b.jpg" class="w3-round w3-center" alt="Random Name" style="width:60%">
                        <p>Javier Herrera</p>
                        <p>@localizer["Estudiante"]</p>
                    </div>
                </div>
            </div>
        </div>

        <!-- The Contact Section -->
        <div class="w3-container w3-content w3-padding-64" style="max-width:800px">
            <img src="https://www.bancocajasocial.com/portalserver/content/atom/ed3567c4-64a3-462a-93a4-7c6466ef50e8/content/8-ASSETS-PRODUCTOS/2.Contenidos-Relacionados-Secundarios/1-Banca-Personas/banco-caja-social-internas-header-Contactenos.jpg?id=10004d9f-b320-4868-bffa-699bf9a9774e" class="w3-opacity-min" style="width:100%">
            <div class="w3-row w3-padding-32">
                 <h2 class="w3-wide w3-center">@localizer["Contacttittle"] </h2>
                <p class="w3-opacity w3-center"><i>@localizer["Contactsubtittle"]</i></p>
                <div class="w3-opacity w3-center">
                    <i class="fa fa-map-marker" style="width:30px"></i> Facatativa, Colombia<br>
                    <i class="fa fa-phone" style="width:30px"></i> @localizer["Cel"]: +053 3143379243<br>
                    <i class="fa fa-envelope" style="width:30px"> </i> @localizer["Email"]: proyectoplantaxx@gmail.com<br>
                </div>
            </div>
        </div>
    </div>


</body>
</html>
